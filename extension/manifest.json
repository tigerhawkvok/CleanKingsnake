{
  "manifest_version": 1,

  "name": "Clean Kingsnake",
  "description": "Remove ads and cruft on Kingsnake.com",
  "version": "1.0",
    "content_scripts": {
        "matches":["https://*.kingsnake.com/*","http://*.kingsnake.com/*"]
        "js":["main.js"]
        "css":[],
        "run_at":"document_end"
    }
  "permissions": [
      "http://*.kingsnake.com",
      "https://*.kingsnake.com"
  ]
}
